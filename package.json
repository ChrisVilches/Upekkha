{
  "name": "custom",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css:prod": "NODE_ENV=production tailwindcss --postcss -i ./input.css -o ./dist/style.css",
    "css:dev": "NODE_ENV=development tailwindcss --postcss -i ./input.css -o ./dist/style.css --watch",
    "js:prod": "NODE_ENV=production esbuild input.js --bundle --outfile=dist/script.js --minify",
    "js:dev": "NODE_ENV=development esbuild input.js --bundle --outfile=dist/script.js --watch",
    "build:prod": "npm run css:prod && npm run js:prod"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "cssnano": "^7.0.4",
    "esbuild": "0.23.0",
    "postcss": "^8.4.39",
    "tailwind-scrollbar": "^3.1.0",
    "tailwindcss": "^3.4.4"
  },
  "dependencies": {
    "flowbite": "^2.4.1",
    "qr-encode": "^0.3.0"
  }
}
